\usepackage{framed,fancybox,eclbkbox}

\newenvironment{EBNF}%
{\VerbatimEnvironment 
\begin{quote}
\begin{breakbox}
\begin{Verbatim}}%
{\end{Verbatim}
\end{breakbox}
\end{quote}}

\newenvironment{FileToPage}[2]%
{
\VerbatimEnvironment 
\def\tabTitle{#1}
\def\tabLabel{#2}
\begin{table}[p]
\begin{quote}
\footnotesize
\bkcounttrue
\begin{breakbox}
\setlength{\baselineskip}{\normalbaselineskip}
\begin{Verbatim}}%
{\end{Verbatim}
\end{breakbox}
\caption{\tabTitle}
\label{\tabLabel}
\end{quote}
\end{table}}

